title

system read
    electrons 10
    nonuniformrandexcits 4ind-weighted-unbound
endsys

calc
    methods
        method vertex fcimc
    endmethods

    seed -7
    load-balance-blocks off

    nmcyc 3500

    tau-values \
        start refdet-connections \
        max 0.02
    tau-search \
        algorithm histogramming 1e-3 \
        stop-condition off

    totalwalkers 20000
    startsinglepart
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    jump-shift
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
endlog
end
