title

system read
    electrons 10
    nonuniformrandexcits pchb \
        singles on-the-fly-heat-bath \
        doubles particle-selection fully-weighted \
        doubles hole-selection spatial-orb-resolved-fast-weighted
    hphf 0
endsys

calc
    methods
    method vertex fcimc
    endmethods

    nmcyc 6500
    seed 8
    totalwalkers 20000

    tau-values \
        start user-defined 0.001 \
        max 0.02
    tau-search \
        algorithm conventional \
        stop-condition var-shift \
        maxwalkerbloom 3.0
    startsinglepart
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    jump-shift
    load-balance-blocks off
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
endlog
end
