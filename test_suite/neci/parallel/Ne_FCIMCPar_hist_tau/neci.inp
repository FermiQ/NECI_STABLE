title

system read
    electrons 10
    nonuniformrandexcits 4ind-weighted
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    nmcyc 3500
    totalwalkers 20000

    tau-values \
        start refdet-connections \
        max 0.02

    tau-search \
        algorithm histogramming 1e-4 \
        stop-condition off

    startsinglepart
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    jump-shift
    load-balance-blocks off
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
endlog
end
