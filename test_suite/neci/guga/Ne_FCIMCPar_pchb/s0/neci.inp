title

system read
    electrons 10
    nonuniformrandexcits guga-pchb
    guga 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    load-balance-interval 1000
    nmcyc 10000
    totalwalkers 10000
    tau-values \
        start user-defined 6.342713037945149E-003 \
        max 0.02
    tau-search \
        algorithm histogramming \
        stop-condition OFF
    startsinglepart 100
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    jump-shift
    load-balance-blocks off
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
endlog
end
