title

system read
    nonuniformrandexcits mol_guga_weighted
    electrons 10
    guga 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    load-balance-interval 1000
    memoryfacspawn 10.0
    memoryfacpart 5.0
    totalwalkers 1000
    tau-values \
        start user-defined 0.001
    startsinglepart
    shiftdamp 0.02
    stepsshift 10
    diagshift 1.0
    nmcyc 500
    truncinitiator
    addtoinitiator 3
    allrealcoeff
    realspawncutoff 0.01
    trial-wavefunction
    pops-trial 20
    semi-stochastic
    pops-core 500
    core-inits
    readpops
    load-balance-blocks off
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
    binarypops
endlog
end
