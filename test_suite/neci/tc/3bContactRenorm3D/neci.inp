system ueg
    electrons   3
    dimension 3
    cell 3 3 3
    nobrillouintheorem
    contactinteractionunitary
    spin-restrict -1
    lattice-excitgen
endsys

calc
    methods
        method vertex fcimc
    endmethods
    nmcyc 2000
    seed        -7
    tau-values \
        start user-defined 1e-4
    totalwalkers       10000
    diagshift       1.5000000000
    shiftdamp       0.1000000000
    stepsshift      10
    memoryfacpart         1.500000000000000
    memoryfacspawn         5.
    truncinitiator
    addtoinitiator        2
    allrealcoeff
    startsinglepart 10
    load-balance-interval 1000
endcalc

logging
    popsfile -1
endlog
end
