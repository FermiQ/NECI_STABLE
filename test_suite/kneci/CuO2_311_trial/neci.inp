title

system read
    freeformat
    electrons 10
    spin-restrict 0
    nonuniformrandexcits 4ind-weighted
    hphf 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    totalwalkers 20000
    startsinglepart 10
    shiftdamp 0.05
    stepsshift 10
    proje-changeref 1.2
    truncinitiator
    addtoinitiator 3
    seed -7
    readpops
    walkcontgrow
    semi-stochastic
    pops-core 200
    core-inits
    trial-wavefunction
    pops-trial 100
    allrealcoeff
    realspawncutoff 0.02
    jump-shift
    tau-values \
        start from-popsfile \
        max 0.02
    tau-search \
        algorithm conventional
    maxwalkerbloom 1
    memoryfacspawn 25.0
    memoryfacpart 5.0
    time 1200.0
    nmcyc 1000
endcalc

integral
endint

logging
    no-append-stats
    popsfile -1
endlog
end
