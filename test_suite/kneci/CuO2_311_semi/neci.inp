title

system read
    freeformat
    electrons 10
    spin-restrict 0
    nonuniformrandexcits 4ind-weighted
    hphf 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    load-balance-interval 1000
    totalwalkers 20000
    startsinglepart 10
    shiftdamp 0.05
    stepsshift 10
    proje-changeref 1.2
    truncinitiator
    addtoinitiator 3
    walkcontgrow
    semi-stochastic 100
    pops-core 100
    core-inits
    allrealcoeff
    realspawncutoff 0.02
    jump-shift

    tau-values \
        start user-defined 0.005 \
        max 0.02
    tau-search \
        algorithm conventional

    maxwalkerbloom 1
    memoryfacspawn 25.0
    memoryfacpart 5.0
    time 1200.0
    nmcyc 600
endcalc

integral
endint

logging
    popsfile -1
endlog
end
