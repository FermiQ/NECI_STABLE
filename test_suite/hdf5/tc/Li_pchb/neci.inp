title

system read noorder
    electrons 3
    nobrillouintheorem
    nonuniformrandexcits pchb \
        singles uniform \
        doubles particle-selection fully-weighted \
        doubles hole-selection fast-weighted
    molecular-transcorr 3-body
endsys

calc
    methods
        method vertex fcimc
    endmethods
    nmcyc 5000
    seed        -7
    totalwalkers       10000
    diagshift       1.5000000000
    shiftdamp       0.1000000000
    stepsshift      10
    tau-values \
        start user-defined 0.001
    tau-search \
        algorithm histogramming \
        stop-condition off
    memoryfacpart         1.50
    memoryfacspawn         5.
    truncinitiator
    addtoinitiator        2
    allrealcoeff
    realspawncutoff 1.
    avgrowthrate
    startsinglepart 10
    definedet 1 2 3
    load-balance-interval 1000
endcalc

integral
    hdf5-integrals
endint

logging
    popsfile -1
endlog
end
