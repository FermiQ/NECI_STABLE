title

  system read
    electrons                          9
    system-replicas                    4
    guga                               1
    nonUniformRandExcits       guga-pchb
    freeformat
    nobrillouintheorem
  endsys

  calc
    seed                   7
    load-balance-blocks  OFF
    methods
      method vertex fcimc
    endmethods

    # general
    semi-stochastic        5
    pops-core            1e3
    readPops
    # store-dets

    # walkers
    allRealCoeff             # required for semi-stochastic initialisation
    truncinitiator
    addtoinitiator         3
    totalWalkers       2.5e4
    realspawncutoff      .30
    maxwalkerbloom         1
    memoryfacspawn      10.0
    memoryfacpart        5.0
    startsinglepart       10

    # shift
    diagshift            .00
    stepsshift            10
    target-shiftdamp     0.5 # default xi = zeta^2/4

    # timestep
    tau                 0.06
    hist-tau-search     0.999999 100000 2000
    max-tau                1
    # min-tau             0.05

    # replicas
    definedet   1 3 5    8 9 11   14 16 18
    # root 1 uuu duu ddd
    # root 2 uuu ddu ddu
    multiple-initial-states
        1 3 5   8  9 11   14 16 18
        1 3 5   8  9 11   14 16 18
        1 3 5   8 10 11   14 16 17
        1 3 5   8 10 11   14 16 17
    orthogonalise-replicas
    replica-single-det-start
    global-core-space
  endcalc

  logging
    RDMlinspace                 10 2 50
    hdf5-pops-read
    popsfile                         -1
    full-core-rdms
    print-molcas-rdms
  endlog

end
