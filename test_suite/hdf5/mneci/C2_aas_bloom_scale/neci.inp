title

system read
    electrons 8
    nonuniformrandexcits pchb \
        singles uniform \
        doubles particle-selection full-full \
        doubles hole-selection fast-fast
    system-replicas 2
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    nmcyc 100
    totalwalkers 10000
    memoryfacspawn 10
    definedet 1-6 9-10
    shiftdamp 0.2
    stepsshift 1
    auto-adaptive-shift
    readpops

    tau-values \
        start from-popsfile

    tau-search \
        algorithm conventional \
        stop-condition off
endcalc

logging
    popsfile -1
    hdf5-pops
endlog
end
