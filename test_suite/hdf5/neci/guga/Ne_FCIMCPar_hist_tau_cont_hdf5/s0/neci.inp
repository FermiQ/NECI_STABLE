title

system read
    electrons 10
    nonuniformrandexcits mol_guga_weighted
    guga 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    nmcyc 1000
    totalwalkers 1000
    tau-values \
        start from-popsfile \
        max 0.02
    tau-search OFF

    readpops
    startsinglepart 100
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    jump-shift
    load-balance-blocks off
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2 0
endint

logging
    popsfile -1
    hdf5-pops-read
    no-append-stats
endlog
end
