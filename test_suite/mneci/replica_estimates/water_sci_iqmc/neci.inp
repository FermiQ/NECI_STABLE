title

system read noorder
    molpromimic
    symignoreenergies
    freeformat
    electrons 8
    spin-restrict 0
    sym 0 0 0 0
    nonuniformrandexcits 4ind-weighted
    nobrillouintheorem
    hphf 0
    system-replicas 2
endsys

calc
    methods
        method vertex fcimc
    endmethods
    memoryfacpart 10.0
    memoryfacspawn 100.0
    memoryfachash 5.0
    stepsshift 1
    nmcyc 20
    seed 7611
    shiftdamp 0.05
    maxwalkerbloom 3
    allrealcoeff
    realspawncutoff 0.0
    averagemcexcits 10.0
    en2-initiator
    tau-values \
        start user-defined 0.1
    tau-search \
        algorithm conventional
    semi-stochastic
    read-core
    global-core-space
    pure-initiator-space
    read-initiator
    precond 50000
endcalc

integral
endint

logging
    replica-estimates
    paired-replicas
    popsfile -1
endlog
end
