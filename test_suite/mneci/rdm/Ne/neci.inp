title

system read
    electrons 10
    nonuniformrandexcits 4ind-weighted
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    nmcyc 3700
    totalwalkers 20000
    tau-values \
        start user-defined 0.001
    tau-search \
        algorithm conventional
    startsinglepart
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    load-balance-blocks off
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2  0
endint

logging
    popsfile -1
    calcrdmonfly 3 200 200
endlog
end
