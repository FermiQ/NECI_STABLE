title

system read
    electrons 10
    nonuniformrandexcits 4ind-weighted
    hphf 0
endsys

calc
    methods
        method vertex fcimc
    endmethods
    seed -7
    nmcyc 1500
    totalwalkers 20000
    tau-values \
        start user-defined 0.001
    tau-search \
        algorithm conventional
    startsinglepart 100
    truncinitiator
    addtoinitiator 3
    shiftdamp 0.03
    stepsshift 25
    allrealcoeff
    realspawncutoff 0.05
    semi-stochastic 150
    pops-core 2000
    core-inits
    trial-wavefunction 150
    pops-trial 100
    load-balance-blocks off
    maxwalkerbloom 3.0
endcalc

integral
    freeze 2  0
endint

logging
    popsfile -1
    calcrdmonfly 3 200 1
endlog
end
