[main]
fc = pathf95
cc = pathCC
ld = pathf95
ldflags = -m64
libs = -L ~/src/lib/fftw-3.1.2/lib -L ~/local/lib/pathscale/fftw-3.1.2/lib -lfftw3 -lacml -lstdc++
cpp = cpp -C -traditional -D__PTR64 -D__PATHSCALE__
cppflags =  -D__Linux -DPOINTER8
compiler = pathf95-on-LINUX
module_flag = -module

[dbg]
fflags = -r8 -g -m64 -fno-second-underscore

[opt]
fflags = -r8 -O3 -m64 -fno-second-underscore
