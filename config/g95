[main]
fc = g95
cc = g++
ld = g95
ldflags = -m64
compiler = G95-on-LINUX
cpp = cpp -C -traditional
cppflags = -D__Linux -DPOINTER8 -D__INT64 -DDISABLE_FFTW -D__G95__
libs = -llapack -lblas -lstdc++ -lrt 
module_flag = -I -fmod=

[dbg]
fflags = -g -m64 -cpp -d8 
cflags = -g -m64 
f90flags = -fbounds-check

[opt]
fflags = -O4 -m64 -cpp -d8
cflags = -O -m64 
