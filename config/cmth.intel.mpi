[main]
fc = mpif90
cc = mpiCC
ld = mpif90
ldflags =  -rdynamic
compiler = INTEL-ifort9-on-LINUX
cpp = cpp -C -traditional
cppflags = -D__Linux -DPOINTER8 -DPARALLEL -DHAVE_SSE2 -D__INT64
libs = -lfftw3 -lacml -cxxlib -lm
module_flag = -module

[dbg]
fflags = -g -traceback 
cflags = -g -traceback
f90flags = -CB

[opt]
fflags = -O3
cflags = -O
