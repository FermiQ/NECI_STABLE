[main]
fc = ftn
f90flags = -Mfree 
compiler = PGI-pgf95-on-LINUX
cc = CC
ccd = g++
cpp = cpp -C -traditional
cppflags =  -D__Linux -DPOINTER8 -DPARALLEL -DHAVE_SSE2
ld = ftn
libs =  -pgcpplibs
module_flag = -module

[dbg]
fflags = -g
cflags = -m64 -g -sb -C -errtags

[opt]
fflags = -fastsse -Minfo -Mneginfo
cflags = -m64 -O2
